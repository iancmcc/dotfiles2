# Virtualenvwrapper
source $(which virtualenvwrapper.sh)

# The next line updates PATH for the Google Cloud SDK.
source '/Users/ian/src/google-cloud-sdk/path.bash.inc'

# The next line enables bash completion for gcloud.
source '/Users/ian/src/google-cloud-sdk/completion.bash.inc'

function gmj() {
    source $HOME/src/gmj/bin/activate
    cd $HOME/src/gmj/gmj-server
}
